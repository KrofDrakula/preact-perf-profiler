{
  "name": "preact-perf-profiler",
  "version": "1.0.0",
  "description": "A HOC for measuring rendering performance with Preact",
  "main": "dist/preact-perf-profiler.cjs.js",
  "module": "dist/preact-perf-profiler.esm.js",
  "browser": "dist/preact-perf-profiler.umd.js",
  "scripts": {
    "test": "ava",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pretest": "npm run build",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krofdrakula/preact-perf-profiler.git"
  },
  "keywords": [
    "preact",
    "performance"
  ],
  "author": "Klemen Slaviƒç <krof.drakula@gmail.com> (https://krofdrakula.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krofdrakula/preact-perf-profiler/issues"
  },
  "homepage": "https://github.com/krofdrakula/preact-perf-profiler#readme",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ],
        "plugins": [
          [
            "transform-react-jsx",
            {
              "pragma": "h"
            }
          ]
        ]
      }
    },
    "require": [
      "./test/_register.js"
    ]
  },
  "devDependencies": {
    "@babel/register": "7.0.0-beta.40",
    "ava": "1.0.0-beta.3",
    "babel-plugin-transform-react-jsx": "7.0.0-beta.3",
    "babel-preset-env": "7.0.0-beta.3",
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "preact": "8.2.7",
    "prettier": "1.11.1",
    "rollup": "0.56.5",
    "rollup-plugin-commonjs": "9.0.0",
    "rollup-plugin-node-resolve": "3.2.0",
    "sinon": "4.4.2",
    "undom": "0.3.3"
  }
}
